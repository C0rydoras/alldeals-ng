{
  "name": "solid",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --bun x vite",
    "build": "bun --bun x vite build",
    "preview": "bun --bun x vite preview",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
